---
import DashboardLayout from '@/components/layouts/DashboardLayout.astro';
import UsersPageWrapper from '@/components/pages/wrappers/UsersPageWrapper';
---

<DashboardLayout title="Gestión de Usuarios">
  <div class="max-w-7xl mx-auto">
    <div class="mb-6">
      <h1 class="text-2xl font-bold text-foreground">Gestión de Usuarios</h1>
      <p class="text-sm text-muted-foreground mt-1">
        Administra usuarios y sesiones del sistema
      </p>
    </div>

    <UsersPageWrapper client:load />
  </div>
</DashboardLayout>
