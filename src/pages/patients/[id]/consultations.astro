---
import DashboardLayout from '@/components/layouts/DashboardLayout.astro';
import PatientConsultationsPageWrapper from '@/components/pages/wrappers/PatientConsultationsPageWrapper';

const { id } = Astro.params;

if (!id) {
  return Astro.redirect('/patients');
}
---

<DashboardLayout title="Consultas del Paciente">
  <PatientConsultationsPageWrapper client:load patientId={id} />
</DashboardLayout>
